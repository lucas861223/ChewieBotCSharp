<Window x:Class="ChewieBot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChewieBot"
        xmlns:CefSharp="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
        mc:Ignorable="d"
        Title="ChewieBot" Width="1366" Height="768">
    <DockPanel>
        <ListView Name="SongList" DockPanel.Dock="Top" Height="300" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                
        </ListView>
        <CefSharp:ChromiumWebBrowser Name="YoutubeEmbed" DockPanel.Dock="Bottom" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"></CefSharp:ChromiumWebBrowser>
    </DockPanel>
</Window>
